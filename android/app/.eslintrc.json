{
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": [
    "airbnb",
    "plugin:react-native/all"
  ],
  "plugins": [
    "react",
    "react-native",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "no-use-before-define": 0
  }
}
